{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": { "cache": false },
    "docs#dev": { "cache": false },
    "example-simple#dev": { "cache": false },
    "example-bookstall#dev": { "cache": false },
    "example-websocket#dev": { "cache": false },
    "@rtrampox/trpc-sveltekit#build": { "outputs": ["dist/**"] },
    "docs#build": { "dependsOn": ["@rtrampox/trpc-sveltekit#build"], "outputs": ["build/**"] },
    "preview": { "dependsOn": ["^build"] },
    "lint": { "outputs": [] },
    "format": { "outputs": [] }
  }
}
